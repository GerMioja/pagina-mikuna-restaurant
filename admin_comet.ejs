<body>
    <h1>coment admin</h1>
    <table>
        <tbody>
            <% if (comentario) { %>
                <% for (var i = 0; i < comentario.length; i++) { %>
                    <tr>
                        <td><%= (i + 1) %></td>
                        <td><%= comentario[i].nombre %></td>
                        <td><%= comentario[i].comentario %></td>
                    </tr>
              <%  } %>
            <%  } %>
        </tbody>
    </table>
</body>
</html>